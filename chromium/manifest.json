{
   "action": {
      "default_icon": {
         "19": "assets/icon_19.png",
         "38": "assets/icon_38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Tab Sorter"
   },
   "background": {
      "service_worker": "background.js"
   },
